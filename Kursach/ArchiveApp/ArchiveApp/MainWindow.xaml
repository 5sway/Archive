<Window x:Class="ArchiveApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArchiveApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="1200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="0*"/>
        </Grid.RowDefinitions>
        <Grid Background="#bae3e8" Grid.Row="0" Grid.ColumnSpan="2"></Grid>
        <Grid Background="#bae3e8" Width="180" Height="1000" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="1" Panel.ZIndex="-1"></Grid>
        <TextBlock Text='АИС "АРХИВ"' FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="19,0,0,0" Height="20" Width="87" Grid.Column="1"/>
        <Button x:Name="MenuBtn" Background="#bae3e8" Grid.Row="1" Width="180" Height="50" VerticalAlignment="Top" HorizontalAlignment="Center" BorderThickness="0"/>
        <Button x:Name="MenuBtn2" Background="#bae3e8" Grid.Row="1" Width="180" Height="50" VerticalAlignment="Top" Margin="0,50,0,0" HorizontalAlignment="Center" BorderThickness="0"/>
        <Button x:Name="MenuBtn3" Background="#bae3e8" Grid.Row="1" Width="180" Height="50" VerticalAlignment="Top" Margin="0,100,0,0" HorizontalAlignment="Center"  BorderThickness="0"/>
        <Button x:Name="MenuBtn4" Background="#bae3e8" Grid.Row="1" Width="180" Height="50" VerticalAlignment="Top" Margin="0,150,0,0" HorizontalAlignment="Center" BorderThickness="0"/>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" Grid.Column="1" Name="MainFrame"/>
    </Grid>
</Window>
